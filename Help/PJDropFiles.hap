\par \plain\f1\fs18\cf3 Determines the type of file the filter acts upon\plain\f0\fs18\cf3 .
\par 
\par \plain\f2\fs18\cf1\b \protect <KP 1>\plain\f2\fs18\cf3\b property\plain\f2\fs18\cf3  \plain\f3\fs18\cf3 Style\plain\f2\fs18\cf3 : \plain\f2\fs18\cf1 \protect <HS 3>\plain\f3\fs18\cf3 TPJExtFileFilterStyle\plain\f3\fs18\cf1 \protect </HS 3>\plain\f2\fs18\cf3 ;\plain\f2\fs18\cf1 \protect </KP 1>\plain\f0\fs18\cf3 
